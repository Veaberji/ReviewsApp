<script src="https://cdnjs.cloudflare.com/ajax/libs/dropzone/5.9.3/dropzone.min.js"
        type="text/javascript"></script>
<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js"
        integrity="sha256-VazP97ZCwtekAsvgPBSUwPFKdrwD3unUfSGVYrahUqU="
        crossorigin="anonymous"></script>

<div class="mb-5 position-relative pe-0">
    <form id="images-dropzone"
          asp-controller="Image"
          asp-action="UploadImages"
          class="dropzone rounded"
          enctype="multipart/form-data"></form>
    <div id="clear-dropzone"
         class="btn btn-secondary btn-close btn-warning position-absolute top-0 end-0"
         title="Delete all images">
    </div>
</div>
@{
    await Html.RenderPartialAsync("ReviewPartials/_AutocompleteTag");
    await Html.RenderPartialAsync("ReviewPartials/_Dropzone");
}